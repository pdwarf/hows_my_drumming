<%= form_with(model: [drum_session, Reaction.new], local: false) do |form| %>
    <% Reaction.emojis.keys.each do |emoji| %>
      <%= form.button emoji.humanize, value: emoji, name: "reaction[emoji]", class: "bg-blue-500 hover:bg-blue-700 text-white font-bold py-2 px-4 rounded mr-2 mb-2" %>
    <% end %>
<% end %>